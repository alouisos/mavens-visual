    <script type="text/javascript">
      google.load('visualization', '1', {'packages':['motionchart']});
      google.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Popularity-Measures');
        data.addColumn('date', 'Date');
        data.addColumn('number', 'Number-x');
        data.addColumn('number', 'Number-y');
        data.addColumn('string', 'Type');
        data.addRows([
          ['Views',  new Date ("2014-03-11"), 1, 21, 'Views'],
          ['Views',  new Date ("2014-03-20"), 1, 197, 'Views'],
          ['Views',  new Date ("2014-03-30"), 1, 432, 'Views'],
          ['Views',  new Date ("2014-04-10"), 1, 621, 'Views'],
          ['Views',  new Date ("2014-04-20"), 1, 845, 'Views'],
          ['Views',  new Date ("2014-04-30"), 1, 1016, 'Views'],
          

          ['Shares', new Date ("2014-03-11"), 2, 49, 'Shares'],
          ['Shares', new Date ("2014-03-20"), 2, 290, 'Shares'],
          ['Shares', new Date ("2014-03-30"), 2, 608, 'Shares'],
          ['Shares', new Date ("2014-04-10"), 2, 891, 'Shares'],
          ['Shares', new Date ("2014-04-20"), 2, 1171, 'Shares'],
          ['Shares', new Date ("2014-04-30"), 2, 1397, 'Shares'],
              

          ['Likes', new Date ("2014-03-11"), 3,  18, 'Likes'],
          ['Likes', new Date ("2014-03-20"), 3,  110, 'Likes'],
          ['Likes', new Date ("2014-03-30"), 3,  244, 'Likes'],
          ['Likes', new Date ("2014-04-10"), 3,  384, 'Likes'],
          ['Likes', new Date ("2014-04-20"), 3,  510, 'Likes'],
          ['Likes', new Date ("2014-04-30"), 3,  620, 'Likes']
          
        ]);
        var chart = new google.visualization.MotionChart(document.getElementById('chart_div'));
        var options = {};
 options['state'] = '{"orderedByY":false,"iconType":"VBAR","duration":{"timeUnit":"D","multiplier":1},"xAxisOption":"2","iconKeySettings":[{"key":{"dim0":"Shares"}},{"key":{"dim0":"Likes"}},{"key":{"dim0":"Views"}}],"yZoomedDataMax":3181,"dimensions":{"iconDimensions":["dim0"]},"uniColorForNonSelected":false,"nonSelectedAlpha":0.4,"yZoomedDataMin":0,"yZoomedIn":false,"yAxisOption":"3","showTrails":false,"xZoomedDataMin":1,"colorOption":"4","yLambda":1,"playDuration":15000,"sizeOption":"3","xZoomedIn":false,"xLambda":1,"time":"1988","orderedByX":false,"xZoomedDataMax":3}'

options['width'] = 600;
options['height'] = 400;
options['showSidePanel'] = false;
options['showXMetricPicker'] =false;
options['showYMetricPicker'] = false;

        chart.draw(data, options);
      }
    </script>
